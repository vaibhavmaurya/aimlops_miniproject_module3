{
  "name" : "vm-aimlops-module4-preprocessing",
  "description" : "Preprocessing job for module 4",
  "role" : "arn:aws:iam::135671745449:role/vm-aimlops-module4-role-gluejob-s3",
  "executionProperty" : {
    "maxConcurrentRuns" : 1
  },
  "command" : {
    "name" : "glueetl",
    "scriptLocation" : "s3://vm-aimlops-2023/module_4/glue/script/vm-aimlops-module4-preprocessing.py",
    "pythonVersion" : "3"
  },
  "defaultArguments" : {
    "--enable-metrics" : "true",
    "--enable-spark-ui" : "true",
    "--spark-event-logs-path" : "s3://vm-aimlops-2023/module_4/glue/sparkHistoryLogs/",
    "--enable-job-insights" : "true",
    "--enable-glue-datacatalog" : "true",
    "--enable-continuous-cloudwatch-log" : "true",
    "--job-bookmark-option" : "job-bookmark-disable",
    "--job-language" : "python",
    "--TempDir" : "s3://vm-aimlops-2023/module_4/glue/temporary/"
  },
  "maxRetries" : 1,
  "allocatedCapacity" : 2,
  "timeout" : 10,
  "maxCapacity" : 2.0,
  "glueVersion" : "4.0",
  "numberOfWorkers" : 2,
  "workerType" : "G.1X",
  "executionClass" : "FLEX",
  "sourceControlDetails" : {
    "provider" : "GITHUB",
    "repository" : "aimlops_miniproject_module3",
    "branch" : "development",
    "folder" : "bikeshare_glue_preprocessing"
  }
}